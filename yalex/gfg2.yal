%{
/* Declaraciones de c√≥digo C/C++ que se incluyen una sola vez */
%}

%token A a
%token B c
%token C b
%token D g
%token E f
%token H h
%token WS [ \t\n]+

IGNORE WS

%%
rule tokens =
      a { return 'A' }
    | c { return 'B' }
    | b { return 'C' }
    | g { return 'D' }
    | f { return 'E' }
    | h { return 'H' }
%%