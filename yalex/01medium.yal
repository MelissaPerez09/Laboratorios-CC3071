let delim = [' ''\t''\n']
let ws = delim+
let letter = ['A'-'C''a'-'c']
let digit = ['0'-'2']
let id = letter(letter|digit)*

rule tokens = 
    ws          { return WHITESPACE }
  | id          { return ID }
  | ['a'-'c']   { print range }

