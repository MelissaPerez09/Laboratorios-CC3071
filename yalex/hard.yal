(* Expresiones aritm√©ticas con sumas, restas y multiplicaciones *)

let delim = [' ''\t''\n']
let ws = delim+
let letter = ['A'-'Z''a'-'z']
let digit = ['0'-'9']
let id = letter(letter|digit)*
let number = digit+

rule tokens = parse
    id        { return 'ID' }
  | number    { return 'NUMBER' }
  | '+'       { return 'PLUS' }
  | '-'       { return 'MINUS' }
  | '*'       { return 'TIMES' }
  | '('       { return 'LPAREN' }
  | ')'       { return 'RPAREN' }
