/* Gramática de complejidad media */
/* Sigue la gramática SLR: */
/* E → E + T | T */
/* E → E - T | T */
/* T → T ∗ F | F */
/* F → ( E ) | id | NUMBER*/

%token ID
%token NUMBER
%token PLUS
%token MINUS
%token TIMES
%token LPAREN
%token RPAREN

%%

expression:
    expression PLUS term
  | expression MINUS term
  | term
  ;

term:
    term TIMES factor
  | factor
  ;

factor:
    LPAREN expression RPAREN
  | ID
  | NUMBER
  ;
